#include <stdio.h>
#include <stdlib.h>
int main()
{
    int a,b,c;
    printf("Digite Tres numeros inteiros: ");
    scanf("%d %d %d", &a, &b, &c);
        if ((a==b) && (a==c))
        printf("Os numeros sao iguais");
        else 
        if ((a>b) && (a>c)) 
        printf ("\nO maior número é:  %d", a);
        else 
        if ((b>a) && (b>c)) 
        printf ("\nO maior número é:  %d", b);
        else 
        if ((c>a) && (c>b)) 
        printf ("\nO maior número é:  %d", c);
                                        
}
